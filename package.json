{
  "name": "spreadsync",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "build": "tsc",
    "dev:start": "ts-node build/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "server": "node ./build/index.js",
    "start": "docker-compose start && docker-compose logs -f -t --tail 20 app",
    "restart": "docker-compose stop  && npm run start",
    "stop": "docker-compose stop",
    "bash": "docker-compose run app bash",
    "compose-down": "docker-compose down",
    "compose": "docker-compose up --build",
    "migration:update": "knex --knexfile src/knexfile.ts migrate:latest",
    "migration:rollback": "knex --knexfile src/knexfile.ts migrate:rollback",
    "migration:seed": "knex --knexfile src/knexfile.ts seed:run",
    "env": "gen-env-types .env -o src/env.d.ts -e ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "googleapis": "^39.2.0",
    "helmet": "^3.23.3",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.13",
    "lodash": "^4.17.15",
    "moment": "^2.26.0",
    "morgan": "^1.9.1",
    "nodemailer": "^6.4.8",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.16",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/knex": "^0.16.1",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.10",
    "@types/passport-jwt": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "gen-env-types": "^1.0.4",
    "jest": "^25.3.0",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.0",
    "typescript": "^4.1.2"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
